<div class="centerSection" style="width:1000px;">

  <!--Most List Sidebar  -->
<!--style="height: 200px; right:100px; margin-top:-140px; position:absolute;" -->
<div class="d-flex align-items-end flex-column" style="right:100px; margin-top:-100px; position:absolute;">
  <strong>Gainers</strong>
  <div class="p-2">
    <table class="table">

    <tr>
      <th scope="col">Symbol</th>
      <th scope="col">Last</th>
  
      <th scope="col">Change</th>
     
    </tr>

    {{#each stockMostGainers}}
    <tr>
      
     <th scope="row"> <a href="chart?symbol={{this.symbol}}">{{this.symbol}}</a></th> 
      <td>{{this.close}}</td>
      {{#checkNum change 0}}
      <td style="color:red;">{{this.change}}</td>
      {{else}}
      <td style="color:green;">{{this.change}}</td>
      {{/checkNum}}
 

    </tr>
    {{/each}}
  
</table>

  </div>
  <strong>Losers</strong>
  <div class="mt-auto p-2">
     <table class="table">

    <tr>
      <th scope="col">Symbol</th>
      <th scope="col">Last</th>
      <th scope="col">Change</th>
     
    </tr>

    {{#each stockMostLosers}}
    <tr>
      
   <th scope="row"> <a href="chart?symbol={{this.symbol}}">{{this.symbol}}</a></th> 
      <td>{{this.close}}</td>
      {{#checkNum change 0}}
      <td style="color:red;">{{this.change}}</td>
      {{else}}
      <td style="color:green;">{{this.change}}</td>
      {{/checkNum}}
 

    </tr>
    {{/each}}
  
</table>
  </div>
  <div class="mt-auto p-2">

  </div>
</div>

<!-- For Div Stock Price Center -->
{{#checkNum stockQuote.change 0}}
<div class="p-3 mb-2 bg-danger text-white" style="height: 140px; margin-top:40px;">

  <div class="leftData">
<a href="chart?symbol={{stockQuote.symbol}}" style="text-decoration: none; color:white;">  <h2>{{stockQuote.symbol}}</h2> </a>
  <h3>{{stockQuote.companyName}}</h3>
</div>
  <h1>{{symbols}}</h1>
  <div class="center" style="text-align: center; margin-top:-40px;">
    <p><p>Vol {{stockQuote.latestVolume}} M</p></p>
  </div>
  <div class="priceData" style="float:right; margin-top:-95px;">
    <h2>{{stockQuote.latestPrice}}</h2>
    <p>{{stockQuote.change}}<span> ({{stockQuote.changePercent}}%)</span></p>
    
   
    </div>
 
</div>
{{else}}
<div class="p-3 mb-2 bg-success text-white" style="height: 140px; margin-top:40px;">
  <div class="leftData">
 <a href="chart?symbol={{stockQuote.symbol}}" style="text-decoration: none; color:white;">  <h2>{{stockQuote.symbol}}</h2> </a>
  <h3>{{stockQuote.companyName}}</h3>
</div>
  <h1>{{symbols}}</h1>
  <div class="center" style="text-align: center; margin-top:-40px;">
    <p><p>Vol {{stockQuote.latestVolume}} M</p></p>
  </div>
  <div class="priceData" style="float:right; margin-top:-95px;">
    <h2>{{stockQuote.latestPrice}}</h2>
    <p>{{stockQuote.change}}<span> ({{stockQuote.changePercent}}%)</span></p>
    
   
    </div>
 
</div>
{{/checkNum}}





