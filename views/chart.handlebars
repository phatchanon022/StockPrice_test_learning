<input type="text" class="form-control" aria-label="Small" aria-describedby="inputGroup-sizing-sm" 
id="symbolChart" value="{{stQ.symbol}}" style="display:none; border: none; border-color: transparent; width:65px; text-transform: uppercase;">
<input type="text" class="form-control" aria-label="Small" aria-describedby="inputGroup-sizing-sm" 
id="symbolwatch" value="{{symbolwatch}}" style="display:none; border: none; border-color: transparent; width:65px; text-transform: uppercase;">



<div id="containerChart" class="chart"></div>



<script src="https://code.highcharts.com/highcharts.js"></script>
<script src="https://code.highcharts.com/modules/exporting.js"></script>

<script src="/javascripts/chartChartPage.js"></script>
